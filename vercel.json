{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ],
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    },
    "api/process.py": {
      "maxDuration": 60
    }
  }
}
